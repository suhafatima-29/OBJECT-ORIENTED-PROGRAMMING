#include <iostream>
#include <iomanip>  // for setw()

using namespace std;

int main() {
    int num;
    cout << "Enter a number: ";
    cin >> num;

    int count = 1;
    for (int row = 1; row <= 20; row++) {
        for (int col = 1; col <= 10; col++) {
            cout << setw(5) << num * count;  // setw(5) keeps columns aligned
            count++;
        }
        cout << endl;
    }

    return 0;
}
